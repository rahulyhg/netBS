
<table class="table table-striped basic-renderer" style="border:1px solid #eee">
    <thead>
    <tr>
        {% for header in table.headers %}
            <th>{{ header }}</th>
        {% endfor %}
    </tr>
    </thead>
    <tbody>
    {% for row in table.data %}
        <tr>
            {% for value in row %}
                <td>{{ value|raw }}</td>
            {% endfor %}
        </tr>
    {% endfor %}
    </tbody>
</table>